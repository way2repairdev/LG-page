       <string notr="true">QFrame#loginContainer {
    background: #ffffff;
    border: none;
    border-radius: 16px;
}
/* Inputs */
QLineEdit {
    border: 1px solid #E5E7EB;
    border-radius: 10px;
    padding: 10px 12px;
    background: #F9FAFB;
    color: #111827;
}
QLineEdit:focus {
    border: 2px solid #3F51B5;
    background: #FFFFFF;
}
QLineEdit::placeholder { color: #9AA4B2; }
/* Primary button */
QPushButton#loginButton {
    background: #3F51B5;
    color: #ffffff;
    border: none;
    border-radius: 10px;
    padding: 10px 16px;
    min-height: 40px;
    font-weight: 600;
    font-size: 14px;
}
QPushButton#loginButton:hover { background: #3949A3; }
QPushButton#loginButton:pressed { background: #2F3E8F; }
QPushButton#loginButton:disabled { background: #C7CAE3; color: #6B7280; }
/* Close button */
QPushButton#closeButton {
    background: transparent;
    color: #9AA4B2;
    border: none;
    border-radius: 12px;
}
QPushButton#closeButton:hover { background: #EF4444; color: #ffffff; }
/* Links */
QLabel#recoverLabel { color: #3F51B5; }
QLabel#recoverLabel:hover { text-decoration: underline; }
</string>
